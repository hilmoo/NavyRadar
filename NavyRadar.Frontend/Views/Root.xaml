<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:home="clr-namespace:NavyRadar.Frontend.Views.Home"
    xmlns:main="clr-namespace:NavyRadar.Frontend.Views.Main"
    xmlns:register="clr-namespace:NavyRadar.Frontend.Views.Register"
    xmlns:signin="clr-namespace:NavyRadar.Frontend.Views.SignIn"
    xmlns:vm="clr-namespace:NavyRadar.Frontend.ViewModels">
    <DataTemplate DataType="{x:Type vm:HomeVm}">
        <home:HomeIndex />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SignInVm}">
        <signin:SignInIndex />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RegisterVm}">
        <register:RegisterIndex />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MainVm}">
        <main:MainIndex />
    </DataTemplate>
</ResourceDictionary>